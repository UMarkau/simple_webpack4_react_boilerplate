{
    "globals": {
        "window": true,
        "document": true
    },
    "parser": "babel-eslint",
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
        "import/prefer-default-export": "off",
        "react/button-has-type": false,
        "no-shadow": "off",
        "react/forbid-prop-types": "off",
        "react/jsx-indent": 4
    }
}
